
pos = -1
def search(list,n):
    l = 0
    u = len(list)-1

    while l <= u:
        mid = (l+u)//2
        if list[mid] == n:
            globals()['pos'] = l
            return True
        else:
            if list[mid] < n:
                l = mid+1
            else:
                u = mid-1
    return False

list = [1,3,5,7,9,12,14,17,19,20]

if search(list,12):
    print("value found at",pos+1)
else:
    print("value not found")